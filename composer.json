{
    "name": "sandwich/vies-bundle",
    "description": "Symfony bundle for the Vies Package to validate VAT numbers",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Sandwich BV",
            "email": "info@myonlinestore.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "dragonbe/vies": "1.0.6",
        "symfony/http-kernel": "^2.8",
        "symfony/symfony": "^2.8",
        "symfony/validator": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "Sandwich\\ViesBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandwich\\ViesBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "sort-packages": true,
        "platform": {
            "php": "5.5"
        }
    }
}
