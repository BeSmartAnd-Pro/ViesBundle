{
    "name": "besmartand-pro/vies-bundle",
    "description": "Symfony bundle for the Vies Package to validate VAT numbers",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Sandwich BV",
            "email": "info@myonlinestore.com"
        },
        {
            "name": "BeSmartAnd.Pro",
            "email": "hello@BeSmartAnd.Pro",
            "homepage": "https://BeSmartAnd.Pro"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "dragonbe/vies": "^2.1",
        "ergebnis/composer-normalize": "^2.43",
        "symfony/dependency-injection": "^6 || ^7",
        "symfony/http-kernel": "^6 || ^7",
        "symfony/validator": "^6 || ^7"
    },
    "require-dev": {
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^8.5.19 || ^9.5.8",
        "roave/security-advisories": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sandwich\\ViesBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandwich\\ViesBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true,
        "vendor-dir": "vendor"
    }
}
